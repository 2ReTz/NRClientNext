Ping = powershell.exe -Command "Start-Process powershell -ArgumentList '-NoExit', '-Command', 'ping %NRIPAddress%'"
Shared Folder = explorer.exe \\%NRIPAddress%
Remote Desktop = mstsc.exe /v:%NRIPAddress%
Continuous Ping = powershell.exe -Command "Start-Process powershell -ArgumentList '-NoExit', '-Command', 'ping -t %NRIPAddress%'"
